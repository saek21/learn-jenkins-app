{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJnkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "Learnjenkinsapp",
            "image": "340752802258.dkr.ecr.eu-north-1.amazonaws.com/learnjenkinsapp:#APP_VERSION#",
            "portMappings": [{
                "name": "ngnix-80-tcp",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::340752802258:role/ecsTaskExecutionRole"
}